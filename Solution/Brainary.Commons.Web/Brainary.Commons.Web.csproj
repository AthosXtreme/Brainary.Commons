<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Cronos" Version="0.8.3" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Brainary.Commons\Brainary.Commons.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Resources\DataAnnotations.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>DataAnnotations.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="Resources\DataAnnotations.es.resx">
			<Generator></Generator>
		</EmbeddedResource>
		<EmbeddedResource Update="Resources\DataAnnotations.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>DataAnnotations.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>

</Project>
